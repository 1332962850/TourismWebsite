<configuration>
	<connectionStrings>
		<add name="northwindsqlConnectionString" connectionString="Data Source=.\sqlexpress;Initial Catalog=northwindsql;Integrated Security=True" 
         providerName="System.Data.SqlClient"/>
	</connectionStrings>
	<appSettings>
		<add key="DictionaryFolder" value="bin"/>
	</appSettings>
	<system.web>
		<compilation debug="true" targetFramework="4.0">

      
			<assemblies>
				<add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/></assemblies></compilation>
	</system.web>
</configuration>
